generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model URL {
  id        String      @id
  createdAt DateTime @default(now())
  longURL String 
  shortURL String 
  userId    String?
  user      User?     @relation(fields: [userId], references: [id])
}

model User {
  id   String @id
  URLs URL[]
}
